@model UserRole
@using (Html.BeginForm("Mody", "UserRole", FormMethod.Post, new { @id = "fm", @class = "form-horizontal", role = "form" }))
{
    @Html.HiddenFor(p => p.ID)
	     <div class="form-group">
	     <label class="control-label col-md-3" for="ID">ID</label>
        <div class="col-sm-8">
            @Html.TextBoxFor(p => p.ID, new { @class = "form-control", placeholder ="ID" })
        </div>
    </div>	
	     <div class="form-group">
	     <label class="control-label col-md-3" for="Name">Name</label>
        <div class="col-sm-8">
            @Html.TextBoxFor(p => p.Name, new { @class = "form-control", placeholder ="Name" })
        </div>
    </div>	
	     <div class="form-group">
	     <label class="control-label col-md-3" for="Code">Code</label>
        <div class="col-sm-8">
            @Html.TextBoxFor(p => p.Code, new { @class = "form-control", placeholder ="Code" })
        </div>
    </div>	
	     <div class="form-group">
	     <label class="control-label col-md-3" for="IsAdmin">IsAdmin</label>
        <div class="col-sm-8">
            @Html.TextBoxFor(p => p.IsAdmin, new { @class = "form-control", placeholder ="IsAdmin" })
        </div>
    </div>	
	     <div class="form-group">
	     <label class="control-label col-md-3" for="States">States</label>
        <div class="col-sm-8">
            @Html.TextBoxFor(p => p.States, new { @class = "form-control", placeholder ="States" })
        </div>
    </div>	
	     <div class="form-group">
	     <label class="control-label col-md-3" for="StatesDate">StatesDate</label>
        <div class="col-sm-8">
            @Html.TextBoxFor(p => p.StatesDate, new { @class = "form-control", placeholder ="StatesDate" })
        </div>
    </div>	
	   
}


 